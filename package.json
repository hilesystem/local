{
  "author": "Anton Trofimenko",
  "bugs": {
    "url": "https://github.com/hilesystem/local/issues"
  },
  "contributors": [
    {
      "email": "r37r0m0d3l@protonmail.com",
      "name": "Anton Trofimenko"
    }
  ],
  "dependencies": {
    "@r37r0m0d3l/of": "2.4.6"
  },
  "description": "Filesystem abstraction layer and common function wrappers",
  "devDependencies": {
    "@r37r0m0d3l/rollup-config-pkg": "git+https://github.com/r37r0m0d3l/rollup-config-pkg.git",
    "@types/node": "14.11.2",
    "babel-eslint": "10.1.0",
    "eslint": "7.9.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jsdoc": "30.5.2",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.4.2",
    "npm-check": "5.9.2",
    "prettier": "2.1.2",
    "remark-cli": "8.0.1",
    "remark-lint": "7.0.1",
    "remark-preset-lint-consistent": "3.0.1",
    "remark-preset-lint-markdown-style-guide": "4.0.0",
    "remark-preset-lint-recommended": "4.0.1"
  },
  "engines": {
    "node": ">=12.9.0"
  },
  "engineStrict": true,
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "node": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
      },
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/index.cjs",
    "dist/index.cjs.map",
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.mjs",
    "dist/index.mjs.map"
  ],
  "jest": {
    "verbose": true
  },
  "keywords": [
    "@hilesystem",
    "file system",
    "file",
    "fs"
  ],
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "name": "@hilesystem/local",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hilesystem/local.git"
  },
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "build:watch": "rollup --config rollup.config.mjs --watch",
    "lint": "eslint ./src/**/*.js",
    "lint:md": "remark .",
    "package:upgrade": "npx npm-check --update",
    "test": "jest",
    "test:coverage": "jest --collect-coverage"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "0.1.27"
}
