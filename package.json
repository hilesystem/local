  "description": "Filesystem abstraction layer and common function wrappers",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-private-methods": "7.8.3",
    "@babel/preset-env": "7.9.5",
    "@rollup/plugin-commonjs": "11.1.0",
    "@rollup/plugin-node-resolve": "7.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.3",
    "jest": "25.3.0",
    "prettier": "2.0.4",
    "remark-cli": "8.0.0",
    "remark-lint": "7.0.0",
    "remark-preset-lint-consistent": "3.0.0",
    "remark-preset-lint-markdown-style-guide": "3.0.0",
    "remark-preset-lint-recommended": "4.0.0",
    "rollup": "2.6.1",
    "rollup-plugin-babel": "4.4.0",
    "rollup-plugin-terser": "5.3.0"
  },
  "engines": {
    "node": ">=12.9.0"
  },
  "engineStrict": true,
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/local.mjs",
        "require": "./dist/local.cjs"
      },
      "node": {
        "import": "./dist/local.mjs",
        "require": "./dist/local.cjs"
      },
      "import": "./dist/local.mjs",
      "require": "./dist/local.cjs",
      "default": "./dist/local.js"
    }
  },
  "files": [
    "dist/local.cjs",
    "dist/local.cjs.map",
    "dist/local.d.ts",
    "dist/local.js",
    "dist/local.js.map",
    "dist/local.mjs",
    "dist/local.mjs.map"
  ],
  "keywords": [
    "@hilesystem",
    "file system",
    "file",
    "fs"
  ],
  "license": "MIT",
  "main": "./dist/local.cjs",
  "module": "./dist/local.mjs",
  "name": "@hilesystem/local",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hilesystem/local.git"
  },
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "lint": "eslint ./src/**/*.mjs",
    "lint-md": "remark .",
    "test": "jest",
    "upgrade": "yarn upgrade-interactive --latest"
  },
  "type": "module",
  "types": "./dist/local.d.ts",
  "version": "0.0.10"
}
